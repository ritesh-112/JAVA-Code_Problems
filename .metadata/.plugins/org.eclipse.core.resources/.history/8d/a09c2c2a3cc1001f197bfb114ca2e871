package root;

// 4. Find the largest among three numbers.
public class FindLargestNumber {

	public static void main(String[] args) {
		int a = 40;
		int b = 20;
		int c = 40;
		
		if (a>b) {
			System.out.println("A is largest");
		} else if(b>a) {
			System.out.println("B is largest");
		}else {
			System.out.println("C is largest");
		}
		
		
	}

}
