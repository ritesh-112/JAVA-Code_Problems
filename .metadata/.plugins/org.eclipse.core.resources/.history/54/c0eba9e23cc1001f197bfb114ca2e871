package root;

import java.util.Scanner;

// 5. Swap two numbers without using a third variable.
public class SwappingValues {

	public static void main(String[] args) {
		int num1, num2;
		Scanner sc  = new Scanner(System.in);
		System.out.println("Emter 1st no");
		num1 = sc.nextInt();
		System.out.println("Emter 1st no");
	}

}
