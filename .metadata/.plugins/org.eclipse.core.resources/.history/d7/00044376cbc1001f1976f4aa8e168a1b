package root;

import java.util.Scanner;

// 10. Find the reverse of a number.
public class ReversingNumber {

	public static void main(String[] args) {
		try (Scanner sc = new Scanner(System.in)) {
			int num = 11;
			while (num >= 9) {
				int digit = num % 10;
				
				System.out.println(digit);
			}
		}
	}

}
